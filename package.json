{
  "name": "profile",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently -n path,next --hide path \"pnpm dev:path\" \"pnpm dev:next\"",
    "dev:next": "next dev",
    "dev:path": "pathpida -p .gitignore -o app -ws",
    "dev:turbo": "next dev --turbo",
    "build": "pathpida -p .gitignore -o app -s && next build",
    "t:dev": "turbo dev",
    "t:build": "turbo build",
    "start": "next start",
    "lint": "eslint . --fix",
    "update:latest": "pnpm update --latest --interactive",
    "update:env": "vercel env pull .env.local"
  },
  "dependencies": {
    "@next/font": "13.1.6",
    "@react-three/drei": "9.56.5",
    "@react-three/fiber": "8.10.1",
    "@vercel/analytics": "0.1.8",
    "clsx": "1.2.1",
    "leva": "0.9.34",
    "next": "13.1.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-use": "17.4.0",
    "three": "0.149.0"
  },
  "devDependencies": {
    "@types/node": "18.11.18",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "@types/three": "0.148.1",
    "autoprefixer": "10.4.13",
    "concurrently": "7.6.0",
    "daisyui": "2.49.0",
    "eslint": "8.32.0",
    "eslint-config-next": "13.1.6",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-prettier": "4.2.1",
    "pathpida": "0.20.1",
    "postcss": "8.4.21",
    "prettier": "2.8.3",
    "tailwindcss": "3.2.4",
    "turbo": "1.7.0",
    "typescript": "4.9.4",
    "vercel": "28.14.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "react": ">=18",
        "react-dom": ">=18"
      }
    }
  },
  "engines": {
    "node": ">=18",
    "pnpm": ">=7"
  }
}