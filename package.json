{
  "name": "profile",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently -n path,next --hide path \"pnpm dev:path\" \"pnpm dev:next\"",
    "dev:next": "next dev",
    "dev:path": "pathpida -p .gitignore -o app -ws",
    "dev:turbo": "next dev --turbo",
    "build": "pathpida -p .gitignore -o app -s && next build",
    "t:dev": "turbo dev",
    "t:build": "turbo build",
    "start": "next start",
    "lint": "eslint . --fix",
    "update:latest": "pnpm update --latest --interactive",
    "update:env": "vercel env pull .env.local"
  },
  "dependencies": {
    "@react-three/drei": "9.89.3",
    "@react-three/fiber": "8.15.12",
    "@vercel/analytics": "1.1.1",
    "clsx": "2.0.0",
    "leva": "0.9.35",
    "next": "14.0.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-use": "17.4.2",
    "three": "0.159.0"
  },
  "devDependencies": {
    "@types/node": "20.10.3",
    "@types/react": "18.2.41",
    "@types/react-dom": "18.2.17",
    "@types/three": "0.159.0",
    "@typescript-eslint/eslint-plugin": "6.13.1",
    "@typescript-eslint/parser": "6.13.1",
    "autoprefixer": "10.4.16",
    "concurrently": "8.2.2",
    "daisyui": "4.4.17",
    "eslint": "8.55.0",
    "eslint-config-next": "14.0.3",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.0.1",
    "pathpida": "0.22.0",
    "postcss": "8.4.32",
    "prettier": "3.1.0",
    "tailwindcss": "3.3.5",
    "turbo": "1.10.16",
    "typescript": "5.3.2",
    "vercel": "32.6.1"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "react": ">=18",
        "react-dom": ">=18"
      }
    }
  },
  "engines": {
    "node": ">=18",
    "pnpm": ">=7"
  }
}