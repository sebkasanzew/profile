{
  "name": "profile",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently -n path,next --hide path \"pnpm dev:path\" \"pnpm dev:next\"",
    "dev:next": "next dev",
    "dev:path": "pathpida -p .gitignore -o app -ws",
    "dev:turbo": "next dev --turbo",
    "build": "pathpida -p .gitignore -o app -s && next build",
    "t:dev": "turbo dev",
    "t:build": "turbo build",
    "start": "next start",
    "lint": "eslint . --fix",
    "update:latest": "pnpm update --latest --interactive",
    "update:env": "vercel env pull .env.local"
  },
  "dependencies": {
    "@next/font": "13.2.4",
    "@react-three/drei": "9.58.5",
    "@react-three/fiber": "8.12.0",
    "@vercel/analytics": "0.1.11",
    "clsx": "1.2.1",
    "leva": "0.9.34",
    "next": "13.2.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-use": "17.4.0",
    "three": "0.150.1"
  },
  "devDependencies": {
    "@types/node": "18.15.10",
    "@types/react": "18.0.30",
    "@types/react-dom": "18.0.11",
    "@types/three": "0.149.0",
    "autoprefixer": "10.4.14",
    "concurrently": "7.6.0",
    "daisyui": "2.51.5",
    "eslint": "8.36.0",
    "eslint-config-next": "13.2.4",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "4.2.1",
    "pathpida": "0.20.1",
    "postcss": "8.4.21",
    "prettier": "2.8.7",
    "tailwindcss": "3.2.7",
    "turbo": "1.8.5",
    "typescript": "5.0.2",
    "vercel": "28.18.1"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "react": ">=18",
        "react-dom": ">=18"
      }
    }
  },
  "engines": {
    "node": ">=18",
    "pnpm": ">=7"
  }
}