{
  "name": "profile",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently -n path,next --hide path \"pnpm dev:path\" \"pnpm dev:next\"",
    "dev:next": "next dev",
    "dev:path": "pathpida -p .gitignore -o app -ws",
    "dev:turbo": "next dev --turbo",
    "build": "pathpida -p .gitignore -o app -s && next build",
    "t:dev": "turbo dev",
    "t:build": "turbo build",
    "start": "next start",
    "lint": "eslint . --fix",
    "update:latest": "pnpm update --latest --interactive",
    "update:env": "vercel env pull .env.local"
  },
  "dependencies": {
    "@next/font": "13.4.7",
    "@react-three/drei": "9.77.3",
    "@react-three/fiber": "8.13.3",
    "@vercel/analytics": "1.0.1",
    "clsx": "1.2.1",
    "leva": "0.9.35",
    "next": "13.4.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-use": "17.4.0",
    "three": "0.153.0"
  },
  "devDependencies": {
    "@types/node": "18.16.18",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "@types/three": "0.152.1",
    "autoprefixer": "10.4.14",
    "concurrently": "8.2.0",
    "daisyui": "3.1.6",
    "eslint": "8.43.0",
    "eslint-config-next": "13.4.7",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "4.2.1",
    "pathpida": "0.20.1",
    "postcss": "8.4.24",
    "prettier": "2.8.8",
    "tailwindcss": "3.3.2",
    "turbo": "1.10.5",
    "typescript": "5.1.3",
    "vercel": "30.2.3"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "react": ">=18",
        "react-dom": ">=18"
      }
    }
  },
  "engines": {
    "node": ">=18",
    "pnpm": ">=7"
  }
}