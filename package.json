{
	"name": "profile",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "concurrently -n path,next --hide path \"pnpm dev:path\" \"pnpm dev:next\"",
		"dev:next": "next dev",
		"dev:path": "pathpida -p .gitignore -o app -ws",
		"dev:turbo": "next dev --turbo",
		"build": "pathpida -p .gitignore -o app -s && next build",
		"t:dev": "turbo dev",
		"t:build": "turbo build",
		"start": "next start",
		"lint": "eslint . --fix",
		"update:latest": "pnpm update --latest --interactive",
		"update:env": "vercel env pull .env.local"
	},
	"dependencies": {
		"@react-three/drei": "9.107.0",
		"@react-three/fiber": "8.16.8",
		"@vercel/analytics": "1.3.1",
		"clsx": "2.1.1",
		"leva": "0.9.35",
		"next": "14.2.4",
		"react": "18.3.1",
		"react-dom": "18.3.1",
		"react-use": "17.5.0",
		"three": "0.165.0"
	},
	"devDependencies": {
		"@types/node": "20.14.8",
		"@types/react": "18.3.3",
		"@types/react-dom": "18.3.0",
		"@types/three": "0.165.0",
		"@typescript-eslint/eslint-plugin": "7.14.1",
		"@typescript-eslint/parser": "7.14.1",
		"autoprefixer": "10.4.19",
		"concurrently": "8.2.2",
		"daisyui": "4.12.8",
		"eslint": "9.5.0",
		"eslint-config-next": "14.2.4",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-prettier": "5.1.3",
		"pathpida": "0.22.0",
		"postcss": "8.4.38",
		"prettier": "3.3.2",
		"tailwindcss": "3.4.4",
		"turbo": "2.0.5",
		"typescript": "5.5.2",
		"vercel": "34.2.8"
	},
	"pnpm": {
		"peerDependencyRules": {
			"allowedVersions": {
				"react": ">=18",
				"react-dom": ">=18"
			}
		}
	},
	"engines": {
		"node": "20.x",
		"pnpm": ">=9"
	}
}
