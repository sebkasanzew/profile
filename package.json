{
	"name": "profile",
	"version": "0.1.0",
	"private": true,
	"packageManager": "pnpm@10.12.1",
	"scripts": {
		"dev": "concurrently -n path,next --hide path \"pnpm dev:path\" \"pnpm dev:next\"",
		"dev:next": "next dev",
		"dev:path": "pathpida -p .gitignore -o app -ws",
		"dev:turbo": "next dev --turbo",
		"build": "pathpida -p .gitignore -o app -s && next build",
		"t:dev": "turbo dev",
		"t:build": "turbo build",
		"start": "next start",
		"test": "pnpm exec playwright test",
		"pw:open": "pnpm exec playwright test --ui",
		"pw:update": "pnpm exec playwright test --update-snapshots",
		"lint": "eslint . --fix",
		"update:latest": "pnpm update --latest --interactive",
		"update:env": "vercel env pull .env.local"
	},
	"dependencies": {
		"@react-three/drei": "10.3.0",
		"@react-three/fiber": "9.1.2",
		"@vercel/analytics": "1.5.0",
		"clsx": "2.1.1",
		"leva": "0.10.0",
		"next": "15.3.4",
		"react": "19.1.0",
		"react-dom": "19.1.0",
		"react-use": "17.6.0",
		"three": "0.175.0"
	},
	"devDependencies": {
		"@playwright/test": "1.53.1",
		"@tailwindcss/postcss": "4.1.10",
		"@types/node": "22.15.32",
		"@types/react": "19.1.8",
		"@types/react-dom": "19.1.6",
		"@types/three": "0.175.0",
		"@typescript-eslint/eslint-plugin": "8.34.1",
		"@typescript-eslint/parser": "8.34.1",
		"concurrently": "9.1.2",
		"eslint": "8.57.1",
		"eslint-config-next": "15.3.4",
		"eslint-config-prettier": "10.1.5",
		"eslint-plugin-prettier": "5.5.0",
		"pathpida": "0.25.0",
		"postcss": "8.5.6",
		"prettier": "3.5.3",
		"tailwindcss": "4.1.10",
		"turbo": "2.5.4",
		"typescript": "5.8.3",
		"vercel": "43.2.0"
	},
	"pnpm": {
		"peerDependencyRules": {
			"allowedVersions": {
				"react": ">=18",
				"react-dom": ">=18"
			}
		}
	},
	"engines": {
		"node": "20.x || 22.x"
	}
}
